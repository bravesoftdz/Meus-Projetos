#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = gtRBExpD70.bpl gtRBExpD70.bat DCLgtRBExpD70.bpl
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

gtRBExpD70.bpl: gtRBExpD70.dpk
  $(DCC)

DCLgtRBExpD70.bpl: DCLgtRBExpD70.dpk
  $(DCC)

gtRBExpD70.bat: gtRBExpD70.bat
  call $**


