#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = gtRBExpD50.bpl gtRBExpD50.bat DCLgtRBExpD50.bpl
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

gtRBExpD50.bpl: gtRBExpD50.dpk
  $(DCC)

gtRBExpD50.bat: gtRBExpD50.bat
  call $**

DCLgtRBExpD50.bpl: DCLgtRBExpD50.dpk
  $(DCC)


