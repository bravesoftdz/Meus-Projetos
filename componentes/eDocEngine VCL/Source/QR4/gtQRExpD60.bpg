#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = gtQRExpD60.bpl gtQRExpD60.bat DCLgtQRExpD60.bpl
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

gtQRExpD60.bpl: gtQRExpD60.dpk
  $(DCC)

gtQRExpD60.bat: gtQRExpD60.bat
  call $**

DCLgtQRExpD60.bpl: DCLgtQRExpD60.dpk
  $(DCC)


