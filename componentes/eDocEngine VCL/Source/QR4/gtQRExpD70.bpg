#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = gtQRExpD70.bpl gtQRExpD70.bat DCLgtQRExpD70.bpl
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

gtQRExpD70.bpl: gtQRExpD70.dpk
  $(DCC)

DCLgtQRExpD70.bpl: DCLgtQRExpD70.dpk
  $(DCC)

gtQRExpD70.bat: gtQRExpD70.bat
  call $**


