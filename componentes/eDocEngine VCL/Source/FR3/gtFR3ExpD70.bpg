MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = gtFR3ExpD70.bpl gtFR3ExpD70.bat DCLgtFR3ExpD70.bpl
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

gtFR3ExpD70.bpl: gtFR3ExpD70.dpk
  $(DCC)

DCLgtFR3ExpD70.bpl: DCLgtFR3ExpD70.dpk
  $(DCC)

gtFR3ExpD70.bat: gtFR3ExpD70.bat
  call $**


