  object Memo1: TMemo
    Left = 17
    Top = 67
    Width = 435
    Height = 105
    Font.Charset = ANSI_CHARSET
    Font.Color = clWindowText
    Font.Height = -12
    Font.Name = 'Courier New'
    Font.Style = []
    Lines.Strings = (
      'Como Imprimir um Campo tipo MEMO ?'
      ''
      'Este exemplo demonstra como voc'#234' pode imprimir CAMPO MEMO,'
      ''
      '1) Coloque um componente TMEMO no Form,'
      '2) Configure o Tmemo para:'
      '    a) Visible = False'
      '    b) Fonte = "COURIER NEW"'
      '    c) Dimencione a largura para que caiba o conteudo de 1 '
      'linha Conforme a Largura do Formul'#225'rio ou espa'#231'o para '
      'impress'#227'o'
      ''
      '3) Mova o conteudo do DBMemo para o TMemo'
      ''
      '4) Fa'#231'a um LOOP para imprimir o conte'#250'do utilizando a '
      'fun'#231#227'o LINES (Memo1.LINE[x])'
      ''
      'EXEMPLO:'
      ''
      'procedure TForm1.Button4Click(Sender: TObject);'
      'var'
      '     linha : integer;'
      'begin'
      '     rdprint1.Preview         := checkbox1.Checked;'
      '     rdprint1.abrir;'
      '     for linha := 0 to memo1.Lines.Count - 1 do'
      '        begin'
      '        '
      'rdprint1.ImpF(linha+1,2,memo1.lines[linha],[normal]);'
      '        end;'
      '     rdprint1.fechar;'
      'end;')
    ParentFont = False
    ScrollBars = ssVertical
    TabOrder = 0
  end
